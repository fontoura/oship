			misuse = <"">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>

definition
	EVALUATION[at0000.1.1] matches {	-- A check list for specific health conditions in family members
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0004] occurrences matches {1..*} matches {	-- Question group
						items cardinality matches {0..*; unordered} matches {
							CLUSTER[at0002] occurrences matches {1..*} matches {	-- Question
								items cardinality matches {1..2; unordered} matches {
									ELEMENT[at0.7] matches {	-- Condition
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0003.1] matches {	-- Presence
										value matches {
											DV_BOOLEAN matches {
												value matches {True, False}
											}
										}
									}
									ELEMENT[at0.0.11] occurrences matches {0..1} matches {	-- Answer
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0.0.12, 	-- Yes
													at0.0.13, 	-- No
													at0.0.14, 	-- Not asked
													at0.0.15, 	-- Not known
													at0.0.16]	-- Not applicable
												}
											}
										}
									}
									CLUSTER[at0.0.8] occurrences matches {0..*} matches {	-- Affected party
										items cardinality matches {1..2; unordered} matches {
											ELEMENT[at0.0.9] occurrences matches {0..1} matches {	-- Name
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0.0.10] occurrences matches {0..1} matches {	-- Relationship
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {[ac0.0.1]}		-- A family relationship
													}
												}
											}
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- A comment on the answer
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Summary
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

